<%- include('../layout', { title: 'Uploaded Files' }) %>
<body>
  

<section>
  <h2>Uploaded Files</h2>
  <ul>
    <% if (files.length > 0) { %>
      <% files.forEach(file => { %>
        <li>
          <a href="/uploads/<%= file %>" target="_blank"><%= file %></a>
        </li>
      <% }); %>
    <% } else { %>
      <p>No files uploaded yet.</p>
    <% } %>
  </ul>
</section>
</body>